# Routes
# This file defines all application routes (Higher priority routes first)
# ~~~~

# Home page
GET     /                           controllers.Application.index()

# Map static resources from the /public folder to the /assets URL path
GET     /assets/*file               controllers.Assets.at(path="/public", file)

GET		/profile					controllers.Application.profilePage()

GET		/artister					controllers.Application.artister()

GET		/loginPage					controllers.Application.loginPage()

POST	/user						controllers.Application.addUser()

POST	/addTeam					controllers.Application.addTeam()

POST	/addMember					controllers.Application.addTeamMember()

POST	/regCode					controllers.Application.registerCode()

GET		/users						controllers.Application.getUsers()

GET		/signup						controllers.Application.signup()

GET		/logout						controllers.Application.logout()

POST	/login						controllers.Application.login()